lookup c2sc_standard {
    sub [A-Z] by [a.sc-z.sc];
    sub \Idotaccent by \i.TRKsc ;
    sub \uni1e9e by germandbls.sc;
    sub Adieresis by adieresis.sc;
    sub Odieresis by odieresis.sc;
    sub Udieresis by udieresis.sc;
    sub AE by ae.sc;
} c2sc_standard;

feature c2sc {
    lookup c2sc_standard;
} c2sc;
